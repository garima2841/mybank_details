<core:FragmentDefinition xmlns:core="sap.ui.core"
    xmlns="sap.m">
    <Dialog id="moreBankDetails" title="{i18n>confDetail}">
        <VBox id="vBox1" class="sapUiSmallMargin">
            <HBox id="hBox1">
                <Label text="{i18n>accNum}" id="bankAccNum" class="sapUiSmallMarginEnd" />
                <Text text="388473*******" id="bankAccVal" class="sapUiSmallMarginBegin"/>
            </HBox>
            <HBox id="hBox2">
                <Label text="{i18n>accHName}" id="lblAcHolderNameId" class="sapUiSmallMarginEnd" />
                <Text text="Garima Singh" id="idAcHolderNameValue" class="sapUiSmallMarginBegin"/>
            </HBox>
            <HBox id="hBox3">
                <Label text="{i18n>iFSCode}" id="lblIFSCId" class="sapUiSmallMarginEnd" />
                <Text text="000000*******" id="idIFSCValue" class="sapUiSmallMarginBegin"/>
            </HBox>
            <HBox id="hBox4">
                <Label text="{i18n>custID}" id="lblCustId" class="sapUiSmallMarginEnd" />
                <Text text="1234*****" id="idCustValue" class="sapUiSmallMarginBegin"/>
            </HBox>
            <HBox id="hBox5">
                <Label text="{i18n>address}" id="lblAddressId" class="sapUiSmallMarginEnd" />
                <Text text="**/**********" id="idAddressValue" class="sapUiSmallMarginBegin"/>
            </HBox>


        </VBox>
        <beginButton>
            <Button icon="sap-icon://decline" id="onCloseBtn" press="onCloseDialog" text="{i18n>closeBtn}" />
        </beginButton>
    </Dialog>
</core:FragmentDefinition>